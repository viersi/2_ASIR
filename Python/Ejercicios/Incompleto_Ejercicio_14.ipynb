   "source": [
    "num = int(input(\"Escribe un número: \"))\n",
    "\n",
    "if num <= 1:\n",
    "    print(\"no primo\")\n",
    "else:\n",
    "    for i in range(2, num):\n",
    "        if num == i:\n",
    "            print(\"primo\")\n",
    "        else:\n",
    "            if num % i == 0:\n",
    "                print(\"no primo\")\n",
    "            else:\n",
    "                print(\"primo\")"
   ]
